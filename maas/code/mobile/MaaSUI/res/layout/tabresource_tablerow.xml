<?xml version="1.0" encoding="utf-8"?>
<TableRow xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="50dp"
    android:gravity="center" >
    
    <TextView 
        android:id="@+id/tabres_tr_id"
        android:layout_width="100dp"
        android:layout_height="50dp" 
        android:background="@drawable/cellborder_item1"
        android:gravity="center"/>
    
    <TextView 
        android:id="@+id/tabres_tr_name"
        android:layout_width="150dp"
        android:layout_height="50dp" 
        android:background="@drawable/cellborder_item1"
        android:gravity="center"/>
    
    <TextView 
        android:id="@+id/tabres_tr_starttime"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:background="@drawable/cellborder_item1" 
        android:gravity="center"/>
    
    <TextView 
        android:id="@+id/tabres_tr_duration"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/cellborder_item1" 
        android:gravity="center"/>
    
    <LinearLayout 
       	android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/cellborder_item1" >
        
        <ProgressBar
	        android:id="@+id/tabres_tr_progressbar"
	        style="?android:attr/progressBarStyleHorizontal"
	        android:layout_width="40dp"
	        android:layout_height="50dp"
	        android:layout_marginLeft="5dp"
	        android:layout_marginRight="5dp"  />
        
    </LinearLayout>
    
    <TextView 
        android:id="@+id/tabres_tr_state"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:background="@drawable/cellborder_item1" 
        android:gravity="center"/>
    
</TableRow>